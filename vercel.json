{
   "buildCommand": "npm run build:vercel",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "src/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "false",
    "NODE_OPTIONS": "--max-old-space-size=4096"

  }
}